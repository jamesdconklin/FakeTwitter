<h1>Search!</h1>

<!-- <form action="/users/search" method="GET">

</form> -->

<nav class="users-search">
  <input
    type="text"
    name="query"
    value="<%= @query %>"
    data-search-url="<%= search_users_url %>">

  <ul class="users">
    <% @users.each do |user| %>
      <li>
        <%= link_to user.username, user_url(user) %>
        <%= render "follows/form", user: user %>
      </li>
    <% end %>
  </ul>
</nav>
